<Window x:Class="SpaceBattleShotter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceBattleShotter"
        mc:Ignorable="d"
        Title="Space Battle Shooter" Height="615" Width="540">
    <Canvas Name="MyCanvas" Focusable="true" Background="black" KeyDown="OnkeyDown" KeyUp="OnkeyUp">
        <Rectangle Name="player" Fill="Yellow" Height="50" Width="60" Canvas.Left="240" Canvas.Top="525" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Label Name="scoreText" Content="Score: 0" FontSize="18" FontWeight="Bold" Foreground="White"/>
        <Label Name="demageText" Content="Score: 0" FontSize="18" FontWeight="Bold" Foreground="White" HorizontalAlignment="Left" Canvas.Left="453" VerticalAlignment="Center"/>
    </Canvas>
</Window>
